<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linuxboy.eatsafe_actividades">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_orange6"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_orange6_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.linuxboy.eatsafe_actividades.ui.main.MainActivity"></activity>
        <activity
            android:name="com.linuxboy.eatsafe_actividades.ui.login.LoginActivity"
            android:theme="@style/NoActionBar"></activity>
        <activity
            android:name="com.linuxboy.eatsafe_actividades.ui.splash.SplashActivity"
            android:theme="@style/NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.linuxboy.eatsafe_actividades.ui.registro.RegistroActivity"
            android:theme="@style/NoActionBar"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>